package day0108;

public class For_07 {
	public static void main(String[] args) {
		//while for 비교
		
		//while
		System.out.println("while문");
		int i = 10;
		
		while(i >= 0) {
			System.out.print(i-- + " ");
		}
		
		//for
		System.out.println("for문");
		for(int a = 10; a >=0; a--) {
			System.out.println(i + " ");
		}
		
		
		
		
	}
}
