package day0108;

public class QuizForWhile_15 {
	public static void main(String[] args) {
		test1();
		
	}
	
	public static void test1() {
		//문제 1 : 1 ~ 100까지 3의 배수 갯수 출력
		int cnt = 0;
		for(int i = 1; i <= 100; i++) {
			if(i % 3 == 0) cnt++;
		}
		
		System.out.println("3의 배수 갯수 : " + cnt);
	
	}
	
	public static void test2() {
			
	}
	
	public static void test3() {
		
	}
}

