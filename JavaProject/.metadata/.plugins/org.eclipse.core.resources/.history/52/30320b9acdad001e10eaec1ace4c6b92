package day0108;

public class For_07 {
	public static void main(String[] args) {
		//while for 비교
		
		//while
		System.out.println("while문");
		int i = 10;
		
		while(i >= 0) {
			System.out.print(i-- + " ");
		}
		System.out.println();
		
		//for
		System.out.println("for문");
		for(int a = 10; a >= 0; a--) {
			System.out.print(a + " ");
		}
		System.out.println();
		
		System.out.println("for문으로 1 ~ 10");
		for(int a = 1; a <= 10; a++) {
			System.out.print(a + " ");
		}
		System.out.println();
		
		int j = 1;
		while(j <= 10) {
			System.out.print(j++ + " ");
		}
		
	}
}
