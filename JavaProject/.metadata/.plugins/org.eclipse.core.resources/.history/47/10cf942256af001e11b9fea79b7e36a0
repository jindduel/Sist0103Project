package day0110;

public class ArraysChange_03 {
	public static void main(String[] args) {
		int a = 10, b = 20;
		
		System.out.println("a = " + a + ", b = " + b);
		
		int tmp = a;
		a = b;
		b = tmp;

		System.out.println("a = " + a + ", b = " + b);
		
		int[] n = {5, 8, 4};
		
		for(int i : n) System.out.println(i + " ");
	}
}
